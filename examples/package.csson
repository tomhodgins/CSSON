/* Package.json expressed in CSSON with human-friendlier types and comments */
{
  name: '@csson/csson';
  version: '0.0.1';
  description: 'A JSON superset with additional types from CSS';
  main: 'index.js';
  bin: {
    csson: url(cli/node.js)
  };
  scripts: {
    test: 'echo "Error: no test specified" && exit 1'
  };
  repository: {
    type: git,
    url: url(git+https://github.com/tomhodgins/CSSON.git)
  };
  keywords: [
    'CSSON',
    'CSS',
    'JSON'
    'data',
    'format'
  ];
  author: 'Tommy Hodgins';
  license: MIT;
  bugs: {
    url: url(https://github.com/tomhodgins/CSSON/issues)
  };
  homepage: url('https://github.com/tomhodgins/CSSON#readme');
}